arent transparent #ffffff;	-webkit-transform:rotate(360deg);	right: 10px;	display: none;}.left-arrow:hover,.right-arrow:hover {	cursor: pointer;}.owl-demo .item{	border: 2px solid #000000;	/* backgroud */	background: rgb(125,126,125); /* Old browsers */	background: -moz-linear-gradient(top,  rgba(125,126,125,1) 0%, rgba(14,14,14,1) 100%); /* FF3.6+ */	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(125,126,125,1)), color-stop(100%,rgba(14,14,14,1))); /* Chrome,Safari4+ */	background: -webkit-linear-gradient(top,  rgba(125,126,125,1) 0%,rgba(14,14,14,1) 100%); /* Chrome10+,Safari5.1+ */	background: -o-linear-gradient(top,  rgba(125,126,125,1) 0%,rgba(14,14,14,1) 100%); /* Opera 11.10+ */	background: -ms-linear-gradient(top,  rgba(125,126,125,1) 0%,rgba(14,14,14,1) 100%); /* IE10+ */	background: linear-gradient(to bottom,  rgba(125,126,125,1) 0%,rgba(14,14,14,1) 100%); /* W3C */	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#7d7e7d', endColorstr='#0e0e0e',GradientType=0 ); /* IE6-9 */	/* shadow */	-webkit-box-shadow: 4px 4px 5px 0px rgba(50, 50, 50, 0.75);	-moz-box-shadow:    4px 4px 5px 0px rgba(50, 50, 50, 0.75);	box-shadow:         4px 4px 5px 0px rgba(50, 50, 50, 0.75);	border-radius: 10px;	-moz-border-radius: 10px;	-webkit-border-radius: 10px;	margin: 10px;	padding: 30px 0px;	text-align: center;}.owl-demo .item p {	margin-top: 12%;	padding: 10px;	line-height: 30px;	word-wrap: break-word;	/*word-break: break-all;*/}.owl-demo .item p a {	word-break: break-all;}.owl-theme .owl-controls .owl-page span {	background-color: white;}.custom-navigation{	text-align: center;}.custom-navigation a{	khtml-user-select: none;	moz-user-select: none;	ms-user-select: none;	user-select: none;	webkit-tap-highlight-color: #a68653;	webkit-user-select: none;}.call-us{	float: left;	margin-left: 1%;	margin-top: 12%;}.input-group {	min-width: 150px;	margin: auto;}/*stor